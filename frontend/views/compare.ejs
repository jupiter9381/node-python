        <div class="container py-5" ng-app="App" ng-controller="AppController">
          <div class="steps mb-5">
            <ul role="tablist" class="m-auto px-0">
              <li role="tab" class="current col-sm">
                <a>
                  <span class="current-info audible"> </span>
                  <h3 class="title font-weight-bold">1. Upload Bill</h3>
                </a>
              </li>
              <li role="tab" class="col-sm ng-class:{'current':step>1}">
                <a>
                  <h3 class="title font-weight-bold">2. Compare Plan</h3>
                </a>
              </li>
              <li role="tab" class="col-sm ng-class:{'current':step>2}">
                <a>
                  <h3 class="title font-weight-bold">3. Switch Plan</h3>
                </a>
              </li>
            </ul>
          </div>
          <div class="content clearfix">
            <fieldset id="step1" role="tabpanel" class="body" ng-show="step==1">
              <div class="form-group">
                <!-- <label for="firstname">First Name<span class="required">*</span></label> -->
                <input class="form-control" ng-model="first_name" placeholder="First Name">
              </div>
              <div class="form-group">
                <!-- <label for="lastname">Last Name<span class="required">*</span></label> -->
                <input class="form-control" ng-model="last_name" placeholder="Last Name">
              </div>
              <div class="form-group">
                <!-- <label for="email">Email<span class="required">*</span></label> -->
                <input class="form-control" ng-model="mail" placeholder="Email">
              </div>
              <div class="actions clearfix">
                <a href="javascript:void(0)" class="btn btn-primary d-block" ng-click="step=2">Next</a>
              </div>
            </fieldset>

            <fieldset id="step2" role="tabpanel" class="body text-center" ng-show="step==2">
              <div class="clearfix mt-4">
                <div class="row">
                  <div class="col-md-12">
                      <h2 class="text-uppercase">Looking for a better energy plan <span class="red">?</span></h2>
                      <p>We provide comparation of all providers for the most suitable plan for you</p>
                    </div>
                </div>
                <div class="row">
                  <div class="col-md">
                    <a class="btn btn-default text-center" href="ebill">
                      <img class="d-block pb-4 mx-auto" src="/images/ebill.png">
                      <h3>I have my e-bill handy</h3>
                    </a>
                  </div>
                  <div class="col-md">
                    <a class="btn btn-default text-center" href="/postcode">
                      <img class="d-block pb-4 mx-auto" src="/images/postcode.png">
                      <h3>I'll search with my postcode</h3>
                    </a>
                  </div>
                </div>
              </div>
            </fieldset>

            <fieldset id="step4" role="tabpanel" class="body" ng-show="step==4">
              <div class="form-group">
                <label for="firstname">First Name<span class="required">*</span></label>
                <input class="form-control" id="firstname" placeholder="">
              </div>
              <div class="form-group">
                <label for="lastname">Last Name<span class="required">*</span></label>
                <input class="form-control" id="lastname" placeholder="">
              </div>
              <div class="form-group">
                <label for="mobile">Mobile<span class="required">*</span></label>
                <input class="form-control" id="mobile" placeholder="">
              </div>
              <div class="form-group">
                <label for="areacode">Area Code<span class="required">*</span></label>
                <input class="form-control" id="areacode" placeholder="">
              </div>
              <div class="form-group">
                <label for="phone">Phone<span class="required">*</span></label>
                <input class="form-control" id="phone" placeholder="">
              </div>
              <div class="form-group">
                <label for="email">Email<span class="required">*</span></label>
                <input class="form-control" id="email" placeholder="">
              </div>
              <div class="actions text-center mt-5 mb-4">
                <a ng-click="step=5" href="javascript:void(0)" class="btn btn-outline-primary">Next</a>
              </div>
            </fieldset>

            <fieldset id="step5" role="tabpanel" class="body" ng-show="step==5">
              <div class="form-group">
                <label for="postcode">Post Code<span class="required">*</span></label>
                <input class="form-control" id="postcode" placeholder="">
              </div>
              <div class="form-group">
                <label for="supply_address">Energy Supply Address<span class="required">*</span></label>
                <input class="form-control" id="supply_address" placeholder="">
              </div>
              <div class="form-check form-group">
                <input type="checkbox" class="form-check-input" id="same_address">
                <label class="form-check-label" for="same_address">Postal address same as energy supply address</label>
              </div>
              <div class="form-inline form-group">
                <label class="d-inline-block mr-3">Are you about to move into this address?</label>
                <input type="radio" class="form-check-input" id="yes1" name="moveaddress">
                <label for="yes1">Yes</label>
                <input type="radio" class="form-check-input ml-3" id="no1" name="moveaddress">
                <label for="no1">No</label>
              </div>
              <div class="form-inline form-group">
                <label class="d-inline-block mr-3">Do you want solar installed at your property?</label>
                <input type="radio" class="form-check-input" id="yes1" name="moveaddress">
                <label for="yes1">Yes</label>
                <input type="radio" class="form-check-input ml-3" id="no1" name="moveaddress">
                <label for="no1">No</label>
              </div>
              <div class="form-group">
                <label for="email">If you have an electricity ebill handy, please enter your National Meter Identifier(NMI) number?</label>
                <input class="form-control" id="email" placeholder="">
              </div>
              <div class="form-inline form-group">
                <label class="d-inline-block mr-3">Does anyone at your property require life support equipments?</label>
                <input type="radio" class="form-check-input" id="yes1" name="moveaddress">
                <label for="yes1">Yes</label>
                <input type="radio" class="form-check-input ml-3" id="no1" name="moveaddress">
                <label for="no1">No</label>
              </div>
              <div class="actions text-center mt-5 mb-4">
                <a ng-click="step=4" href="javascript:void(0)" class="btn btn-outline-primary">Prev</a>
                <a ng-click="step=6" href="javascript:void(0)" class="btn btn-outline-primary">Next</a>
              </div>
            </fieldset>
  
            <fieldset id="step6" role="tabpanel" class="body" ng-show="step==6">
              <div class="form-group">
                <label for="postcode">What's your date of birth?<span class="required">*</span></label>
                <input class="form-control" id="postcode" placeholder="dd/mm/yyyy">
              </div>
              <div class="form-inline form-group">
                <label class="d-inline-block mr-3">Please provide a form of identification</label>
                <input type="radio" class="form-check-input" id="yes1" name="moveaddress">
                <label for="yes1">Yes</label>
                <input type="radio" class="form-check-input ml-3" id="no1" name="moveaddress">
                <label for="no1">No</label>
              </div>
              <div class="form-inline form-group">
                <label class="d-inline-block mr-3">Do you hold a concession card?</label>
                <input type="radio" class="form-check-input" id="yes1" name="moveaddress">
                <label for="yes1">Yes</label>
                <input type="radio" class="form-check-input ml-3" id="no1" name="moveaddress">
                <label for="no1">No</label>
              </div>
              <div class="actions text-center mt-5 mb-4">
                <a ng-click="step=5" href="javascript:void(0)" class="btn btn-outline-primary">Prev</a>
                <a ng-click="step=7" href="javascript:void(0)" class="btn btn-outline-primary">Next</a>
              </div>
            </fieldset>

            <fieldset id="step7" role="tabpanel" class="body" ng-show="step==7">
              <div class="form-inline form-group">
                <label class="d-inline-block mr-3">Preferred payment method</label>
                <input type="radio" class="form-check-input" id="yes1" name="moveaddress">
                <label for="yes1">Direct Debit</label>
                <input type="radio" class="form-check-input ml-3" id="no1" name="moveaddress">
                <label for="no1">Credit Card</label>
              </div>
              <div class="form-group">
                <label for="postcode">BSB Number</label>
                <input class="form-control" id="postcode" placeholder="xxxxxx00000xx00xx">
              </div>
              <div class="form-group">
                <label for="postcode">Account Number</label>
                <input class="form-control" id="postcode" placeholder="xxxxxx00000xx00xx">
              </div>
              <div class="form-inline form-group">
                <label class="d-inline-block mr-3">Preferred payment frequency</label>
                <input type="radio" class="form-check-input" id="yes1" name="moveaddress">
                <label for="yes1">Monthly</label>
                <input type="radio" class="form-check-input ml-3" id="no1" name="moveaddress">
                <label for="no1">Quaterly</label>
              </div>
              <div class="actions text-center mt-5 mb-4">
                <a ng-click="step=6" href="javascript:void(0)" class="btn btn-outline-primary">Prev</a>
                <a ng-click="step=8" href="javascript:void(0)" class="btn btn-outline-primary">Next</a>
              </div>
            </fieldset>
          </div>

        </div>
    </div>
